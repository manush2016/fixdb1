<p id="notice"><%= notice %></p>

<h1>Listing Configdbs</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>Username</th>
      <th>Password</th>
      <th>Monitor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @configdbs.each do |configdb| %>
      <tr>
        <td><%= configdb.location %></td>
        <td><%= configdb.username %></td>
        <td><%= configdb.password %></td>
        <td><%= configdb.monitor %></td>
        <td><%= link_to 'Show', configdb %></td>
        <td><%= link_to 'Edit', edit_configdb_path(configdb) %></td>
        <td><%= link_to 'Destroy', configdb, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
